<form action="" method="post" id="formModalCarrier">
    <div class="modal" tabindex="-1" role="dialog" id="confirmModalCarrier">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Ajout du numéro de colis</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-group col-lg-12">
                        <label for="new_tracking_value">N°suivi</label>
                        <input type="text" name="tracking_number" id="new_tracking_value" class="form-control" value="{{ objet_carrier.tracking_number }}" />
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="hidden" name="id_order" value="{{ objet.id_order }}" />
                    <button type="button" class="btn btn-themes" id="confirmModalCarrierYes">Ajouter</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</button>
                </div>
            </div>
        </div>
    </div>
</form>