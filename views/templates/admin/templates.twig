    {% endif %}

    {% if draggable %}
        <script src='assets/js/draggable.js'></script>
    {% endif %}

    {% if js_image %}
        <script src='assets/js/image.js'></script>
    {% endif %}

    {% if tree %}
        <script src='assets/js/tree.js'></script>
    {% endif %}

    {% if admin_menu %}
        <script src='assets/js/global.js'></script>
    {% endif %}

    {% if js %}
        <script src='assets/js/{{ js }}.js'></script>
    {% endif %}
